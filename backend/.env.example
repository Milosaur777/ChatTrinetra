# CaptainClaw SaaS Environment Variables

# Server
PORT=3001
NODE_ENV=development

# API Keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
ANTHROPIC_API_KEY=your_claude_api_key_here

# LLM Settings
DEFAULT_MODEL=openrouter/anthropic/claude-haiku-4.5
OLLAMA_ENDPOINT=http://localhost:11434

# File Upload
MAX_FILE_SIZE=50
UPLOAD_DIR=/root/clawd/data/captainclaw/uploads

# Database
DB_PATH=/root/clawd/data/captainclaw/captainclaw.db
